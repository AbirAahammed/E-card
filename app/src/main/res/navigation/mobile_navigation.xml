<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="redbull.ecard.UILayer.cards.CardsFragment"
        android:label="@string/main_screen"
        tools:layout="@layout/fragment_cards" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="redbull.ecard.UILayer.dashboard.ProfileFragment"
        android:label="@string/profile_home"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="redbull.ecard.UILayer.camera.CameraFragment"
        android:label="@string/camera_home"
        tools:layout="@layout/fragment_camera" />
</navigation>